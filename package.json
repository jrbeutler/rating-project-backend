{
  "name": "rating-project-backend",
  "version": "0.0.1",
  "description": "Backend for Rating Project",
  "author": "Eli Sokeland",
  "license": "MIT",
  "keywords": [
    "NestJS",
    "Prisma",
    "Prisma Client",
    "Typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Eli017/rating-project-backend.git"
  },
  "bugs": {
    "url": "https://github.com/Eli017/rating-project-backend/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prisma:introspect": "npx prisma introspect",
    "prisma:save": "npx prisma migrate save --experimental",
    "prisma:up": "npx prisma migrate up --experimental",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "seed": "ts-node prisma/seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "1.1.0",
    "@nestjs/common": "7.4.4",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "7.4.4",
    "@nestjs/graphql": "7.7.0",
    "@nestjs/jwt": "7.1.0",
    "@nestjs/passport": "7.1.0",
    "@nestjs/platform-express": "7.4.4",
    "@nestjs/swagger": "4.6.1",
    "@prisma/client": "2.8.0",
    "apollo-server-express": "2.18.1",
    "bcrypt": "5.0.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "graphql": "15.3.0",
    "graphql-tools": "6.2.4",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "swagger-ui-express": "4.1.4"
  },
  "devDependencies": {
    "@apollo/gateway": "0.21.0",
    "@nestjs/cli": "7.5.1",
    "@nestjs/testing": "7.4.4",
    "@prisma/cli": "2.8.0",
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.1.0",
    "@types/express": "4.17.8",
    "@types/jest": "26.0.14",
    "@types/node": "13.13.5",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "chance": "1.1.7",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.4.2",
    "prettier": "2.1.2",
    "prisma-dbml-generator": "0.3.1",
    "rimraf": "3.0.2",
    "supertest": "5.0.0",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.4",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
